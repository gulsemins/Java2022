package conditions;

public class Main {

	public static void main(String[] args) {
		int sayi =24;
		if (sayi<20) {
			System.out.println("Sayi 20 den küçüktür");
		}
		if (sayi<15) {
			System.out.println("Sayi 15 den küçüktür");
		}else {
			System.out.println("Sayi 20 den küçük değildir");
		}
	}

}
