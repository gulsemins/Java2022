package multiDimensionalArrayDemo;

public class Main {

	public static void main(String[] args) {
		String[][]sehirler = new String [3][3]; // satır,sütün
		
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
		sehirler [0][0] = "İstanbul";
	}

}
