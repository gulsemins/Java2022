		 int ogrenciSayisi = 20;
	        String mesaj="Ögrenci Sayisi:";
	        System.out.println(mesaj + ogrenciSayisi);
	        System.out.println(mesaj + ogrenciSayisi);
	        System.out.println("Öğrenci sayim: " + ogrenciSayisi);
	        System.out.println("Öğrenci sayim: " + ogrenciSayisi);
	        System.out.println("Öğrenci sayım: " + ogrenciSayisi);
	    
	}

